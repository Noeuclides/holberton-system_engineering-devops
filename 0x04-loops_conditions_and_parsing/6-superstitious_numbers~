#!/usr/bin/env bash
# This script is displaying "Holberton School" 10 times but hi at nine

for i in {1..20}; do
    if [ $i -eq 4 ]
    then
	echo $i
	echo "bad luck from China"
    else
    if [ $i -eq 9 ]
    then
	echo $i
	echo "bad luck from Japan"
    else
    if [ $i -eq 17 ]
    then
	echo $i
	echo "bad luck from Italy"
    else
	echo "Holberton School"
    fi
    fi
    fi
done
